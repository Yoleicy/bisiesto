Algoritmo iterationMaster
	//primera forma
	Definir num Como Entero
	Para num <- 1 Hasta 100 Con Paso 1 Hacer
		Imprimir num
	Fin Para
	
	
    //segunda forma 
	//Definir num,contador Como Entero
    contador <- 1
	Para num <- 1 Hasta 100 Hacer
		Imprimir  contador
		contador <- contador + 1
	Fin Para
	
	//tercera forma
	//Definir num Como Entero
    num <- 1
	Mientras num <= 100 Hacer
		Imprimir  num
		num<- num + 1
	Fin Mientras
	
	
	
Fin Algoritmo
